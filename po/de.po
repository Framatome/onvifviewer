#: ../desktop/net.meijn.onvifviewer.appdata.xml.in:105
msgid "Some small improvements"
msgstr "Einige kleine Verbesserungen"

#: ../desktop/net.meijn.onvifviewer.appdata.xml.in:51
msgid "Added support for camera discovery"
msgstr "Unterstützung für Kamera-Ermittlung"

#: ../desktop/net.meijn.onvifviewer.appdata.xml.in:52
msgid "Fixed video viewer for Android"
msgstr "Videobetrachter für Android behoben"

#: ../desktop/net.meijn.onvifviewer.appdata.xml.in:53
msgid "Improved translations"
msgstr "Verbesserte Übersetzungen"

#: ../desktop/net.meijn.onvifviewer.appdata.xml.in:62
#: ../desktop/net.meijn.onvifviewer.appdata.xml.in:73
msgid "Improved support for older devices"
msgstr "Verbesserte Unterstützung für ältere Geräte"

#: ../desktop/net.meijn.onvifviewer.appdata.xml.in:63
msgid "Show multiple cameras if room available"
msgstr "Mehrere Kameras anzeigen, wenn Raum verfügbar ist"

#: ../desktop/net.meijn.onvifviewer.appdata.xml.in:64
msgid "Improved translations and added new langagues"
msgstr "Verbesserte Übersetzungen und neu hinzugefügte Sprachen"

#: ../desktop/net.meijn.onvifviewer.appdata.xml.in:65
#: ../desktop/net.meijn.onvifviewer.appdata.xml.in:75
#: ../desktop/net.meijn.onvifviewer.appdata.xml.in:116
